\name{hazard.weib}

%%%
\alias{hazard.weib}

\title{The Hazard Weibull function}
\description{
  Hazard function for the Weibull model of failure data.
}
\usage{
 hazard.weib(x,alpha,beta,mu=0)
}
\arguments{
  \item{x}{vector of quantiles.}
  \item{alpha}{scale parameter.}
  \item{beta}{shape parameter.}
  \item{mu}{location parameter. If `mu = 0' then it is the Weibull Model of two parameters.}
}
